<% provide(:title, 'Create New Group') %>

<h1>Create a new group:</h1>
<div class="row">
  
    <%= form_for(@group) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>

      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :description %>
      <%= f.text_field :description %>



      <%= f.submit "Create Group!" %>
    <% end %>
  </div>
</div>
